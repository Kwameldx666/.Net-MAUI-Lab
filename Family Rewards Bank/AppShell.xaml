<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Family_Rewards_Bank.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Family_Rewards_Bank"
    Title="Family_Rewards_Bank">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Organizer"
        ContentTemplate="{DataTemplate local:Organizer}"
        Route="Organizer"/>
    
    <ShellContent
        Title="Add data"
        ContentTemplate="{DataTemplate local:AddEventPage}"
        Route="AddEventPage"/>
    <ShellContent
        Route="UpdateEventPage"/>

</Shell>
